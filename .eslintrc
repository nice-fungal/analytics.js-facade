{
  "extends": "eslint-config-airbnb",

  "env": {
    "browser": true
  },

  "rules": {
    "arrow-parens": 0,
    "consistent-return": 0,
    "curly": 0,
    "func-names": 0,
    "guard-for-in": 0,
    "import/first": 0,
    "import/order": 0,
    "import/prefer-default-export": 0,
    "indent": 0,
    "max-len": 0,
    "no-continue": 0,
    "no-else-return": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-prototype-builtins": 0,
    "no-restricted-globals": 0,
    "no-restricted-syntax": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "no-use-before-define": 0,
    "no-useless-escape": 0,
    "no-var": 0,
    "nonblock-statement-body-position": 0,
    "object-curly-newline": 0,
    "object-shorthand": 0,
    "operator-linebreak": 0,
    "prefer-arrow-callback": 0,
    "prefer-const": 0,
    "prefer-rest-params": 0,
    "prefer-spread": 0,
    "prefer-template": 0,
    "quotes": 0,
    "space-before-function-paren": 0,
    "strict": 0,
    "vars-on-top": 0
  }
}
